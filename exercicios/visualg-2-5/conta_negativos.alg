algoritmo "ContaNegativos"
// Função :
// Autor :
// Data : 16/02/2022
// Seção de Declarações 
var
n, c, totn: Inteiro
inicio
// Seção de Comandos
c <- 1
repita
      escreva("Digite um número: ")
      leia(n)
      se (n < 0) entao
         totn <- totn +1
      fimse
      c <- c +1
ate (c > 5)
escreval("Foram digitados",totn," valores negativos.")
fimalgoritmo
