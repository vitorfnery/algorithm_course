algoritmo "antecessor_sucessor"
// Função :
// Autor :
// Data : 02/02/2022
// Seção de Declarações 
var
n,a,s: Inteiro
inicio
// Seção de Comandos
   Escreva("Digita um número: ")
   Leia(n)
   a <- n-1
   s <- n+1
   Escreval("O antecessor de",n," é",a)
   Escreva("O sucessor de",n," é",s)
fimalgoritmo
