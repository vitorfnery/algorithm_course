algoritmo "dobro e terça parte"
// Função :
// Autor :
// Data : 02/02/2022
// Seção de Declarações 
var
   n, d, t: Real
inicio
// Seção de Comandos
   Escreva("Digite um número: ")
   Leia(n)
   d <- n*2
   t <- n/3
   Escreval("O dobro de",n," é",d)
   Escreva("A terça parte de",n," é",t)
fimalgoritmo
