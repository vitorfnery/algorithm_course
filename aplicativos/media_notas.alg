algoritmo "medianotas"
// Função :
// Autor :
// Data : 02/02/2022
// Seção de Declarações 
var
   n1, n2, m: Real
inicio
// Seção de Comandos
   Escreva("Nota 1: ")
   Leia(n1)
   Escreva("Nota 2: ")
   Leia(n2)
   m <- (n1+n2)/2
   Escreva("A média entre",n1," e",n2," é igual a ",m:2:1)
fimalgoritmo
